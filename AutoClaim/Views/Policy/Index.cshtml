@model List<AutoClaim.Models.Policy>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>
<table class="table table-bordered table-hover">
    <tr>
        <th>Policy ID</th>
        <th>First Name</th>
        <th>DateOfBirth</th>
        <th>Vechicle Type</th>
       
    </tr>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@m.Id</td>
            
                <td>@m.DateOfBirth</td>
                <td>@m.FirstName</td>
               
            </tr>
        }
    </tbody>
</table>

