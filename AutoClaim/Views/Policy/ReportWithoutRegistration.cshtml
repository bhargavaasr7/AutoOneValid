@model AutoClaim.Models.Policy
@{
    ViewBag.Title = "ReportWithoutRegistration";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ReportWithoutRegistration</h2>
<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(r => r.Id, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(r => r.Id, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(r => r.Id, "", new { @class = "text-danger" })
        </div>
    </div>
    <br>
    <div class="form-group">
        @Html.LabelFor(r => r.EmailId, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(r => r.EmailId, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(r => r.EmailId, "", new { @class = "text-danger" })
        </div>
    </div>
    <br>
    <div class="form-group">
        @Html.LabelFor(r => r.MobileNo, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(r => r.MobileNo, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(r => r.MobileNo, "", new { @class = "text-danger" })
        </div>
    </div>
</div>

<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="SignIn" class="btn btn-primary" />
        &nbsp;&nbsp;&nbsp;&nbsp;
        <input type="submit" value="Cancel" class="btn btn-warning" />

    </div>
</div>




